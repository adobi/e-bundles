                    //FB.Canvas.setSize({ width: 740, height: 740 });
                      
                    window.fbAsyncInit = function() {
                      FB.init({appId: 138701632858229,
                                  //channelUrl  : 'http://fbtest.invictus.hu/channel.html',
                                  status: true,
                                  cookie: true,  
                                  xfbml: true});  };  
                         (function() {
                             var e = document.createElement('script');
                             e.async = true;  
                             e.src = document.location.protocol +  
                             '//connect.facebook.net/en_US/all.js';
                             document.getElementById('fb-root').appendChild(e);
                    }());
                                   
                    function PostToFb(accToken, desc, picture)
                    {
                        //alert(" picture: " + picture );     
			var picUrl = 'http://fbtest.invictus.hu/TunerGarage/Facebook/Test/' + picture; 
                        FB.ui(
                        {
                             method: 'feed',
                             display: 'popup',
                             user_message_prompt: "Publish This!",
                             message: 'Facebook Dialogs are easy!',
                             caption: '',
                             description: desc,
                             title: 'Tuner Garage',
                             name: 'Tuner Garage',
                             picture: picUrl,
                             redirect_uri: 'http://fbtest.invictus.hu/TunerGarage/Facebook/Work/fbpost.html',
                             access_token: accToken,
                             next: null,
							 show_error: true
                           }/*,
                           function(response) {
                             if (response && response.post_id) {
                               //alert('Post was published.');
                             } else {
                               //alert('Post was not published.');
                             }
                           }*/
                          
                        );
                    }
                    
                    function RequestThroughFb(objectData, _message)
                    {
                        var _objectDatas = objectData.split(";");
                        if( _objectDatas.length < 2 )
                            return;                 
                                                
                         $.ajax({
                            url: "request.php", 
                            dataType: "html",
                            type: 'POST', 
                            data: {
                                cmd : 100,
                                uid : userId,
                                param : _objectDatas[1]
                            },
                            success: function(data){
                                //alert("RequestThroughFb: " + data.toString());
                                FB.ui(
                                    {
                                        method: 'apprequests', 
                                        message: _message, 
                                        data: objectData,
                                        display: 'popup',
                                        title: 'TunerGarage',
                                        redirect_uri : 'http://fbtest.invictus.hu/TunerGarage/Facebook/Work/fbrequest.php',
                                        exclude_ids : data.split(","),
                                        next : null,
                                        show_error: true
                                    }
                                );
                            }
                            ,
                            error : function(error) {
                                //alert("error: " + error.toString());
                            }
                        });
                        
                    }
                    
                    function OnlyOneRequestThroughFb(objectData, _message)
                    {
                        var _objectDatas = _objectDatas.split(";");
                        if( _objectDatas.length < 2 )
                            return;
                        
                         $.ajax({
                            url: "request.php", 
                            dataType: "html",
                            type: 'POST', 
                            data: {
                                cmd : 100,
                                uid : userId,
                                param : _objectDatas[1]
                            },
                            success: function(data){
                                //alert("RequestThroughFb: " + data.toString());
                                FB.ui(
                                    {
                                        method: 'apprequests', 
                                        message: _message, 
                                        data: objectData,
                                        display: 'popup',
                                        title: 'TunerGarage',
                                        redirect_uri : 'http://fbtest.invictus.hu/TunerGarage/Facebook/Work/fbrequest.php',
                                        exclude_ids : data.split(","),
                                        max_recipients : 1,
                                        next : null,
                                        show_error: true
                                    }
                                );
                            }
                            ,
                            error : function(error) {
                                //alert("error: " + error.toString());
                            }
                        });
                    }
                    
                    function Test()
                    {
                          FB.ui(
                                    {
                                        method: 'apprequests', 
                                        message: 'TestMsg', 
                                        data: 'TestData',
                                        display: 'iframe',
                                        title: 'TunerGarage',
                                        redirect_uri : 'http://fbtest.invictus.hu/TunerGarage/Facebook/Work/fbrequest.php',
                                        next : null,
                                        show_error: true
                                    }
                                );
                    }
                        
                </script> 
                <a href="javascript:void(0)" onclick="Test();">TestRequest</a>
