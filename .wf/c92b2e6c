		    $(function() {
               
     			$("#showSimpleModal").click(function() {
     			    
     				//$("div#simpleModal").addClass("show");
     				/*$("#footer, #container, #header").css({
                        'opacity': 0.4

                    });*/
     				//$("div#simpleModal").css('opacity', 1).show();
                    $("div#simpleModal").addClass('shown').show();
     				return false;
     			});
     			
     			
     			$("#closeSimple").click(function() {
     				//$("div#simpleModal").removeClass("show");
                    
     				/*$("#footer, #container, #header").css({
                        'opacity': 1
                    });*/
                    /*
     				$("div#simpleModal").css({
                        'opacity': 0,
                        'z-index': -1
                    });
                    */
                    $("div#simpleModal").removeClass('shown').hide();
     				return false;
     			});		        
		        //if ($('#content').length)
		            //$('.jScrollPaneContainer').css('height', $('#content').height() - $('.game-header').height() +500 );
		            //console.log($('.a-game').height());
                //$('#sidebar').jScrollPane({showArrows:false}); 
                $('.searchbar_search_button').bind('click', function() {
                    $(this).parents('form').submit(); 
                });
                
                if ($.browser.msie && $.browser.version === '7.0') {
                    //alert('hello')
                    $('#content').height($('#content').height()+20);
                }
            });
            $(window).load(function() {
                $('a[rel=images-fancybox]').fancybox({
            		'transitionIn'		: 'elastic',
            		'transitionOut'		: 'elastic',
            		'titlePosition' 	: 'over',
                        'cyclic'            : true
                }); 
		        
                marqueeInit({
                	uniqueid: 'mycrawler',
                	style: {
                		'padding': '2px',
                		'width': '510px', //3276px
                		'height': '40px',
                        'margin-left':'0px',
                        'margin-right': '0px',
                        'border': '0px solid red'
                	},
                	inc: 5, //speed - pixel increment for each iteration of this marquee's movement
                	mouse: 'cursor driven', //mouseover behavior ('pause' 'cursor driven' or false)
                	moveatleast: 2,
                	neutral: 150,
                	savedirection: true
                });
                
		        $('.game-description, .page-content').height(
		            $('#content').outerHeight(true) - 
		            $('.game-header').outerHeight(true) - 
		            $('.inner-nav').outerHeight(true) -40
		        );
		        
		        if (/partners/.test(window.location.href)) {
		            
		            //$('.page-content').height(528);
		        }
		        
		        
                if ($('.game-description').height() > 400) {
                    $('.game-description').height(400);
                }
                
                if( navigator.userAgent.match(/Android/i) ||
                 navigator.userAgent.match(/webOS/i) ||
                 navigator.userAgent.match(/iPhone/i) ||
                 navigator.userAgent.match(/iPod/i)
                ){
                
                    $('.game-description, .page-content').jScrollPane({showArrows:true}); 
                } else {
                    
                    $('.game-description, .page-content').jScrollPane({showArrows:false}); 
                    
                }
                
                
		       /* $('.game-description').height(
		            $('#content').outerHeight(true) - 
		            $('.game-header').outerHeight(true) - 
		            $('.inner-nav').outerHeight(true) -40
		        );
		        */
		        
		        /*
		        console.log('game description: '+$('.game-description').outerHeight(true));
		        
                
                
		        console.log('content: '+$('#content').outerHeight(true));
		        console.log('game header: '+$('.game-header').outerHeight(true));
		        console.log('nav: '+$('.inner-nav').outerHeight(true));
		        */
		        //console.log('scroll pane:'+$('.jScrollPaneContainer').height(400));
		        
                if ($('.gallery-images').length) {

                    $.each($('.page-img-wrapper'), function(i, v) {

                        if ($(v).find('img-title').length) {

                            $(v).height($(v).outerHeight()+10);                            
                        } 
                    });      
                    $('.gallery-images').masonry();

                }
            });
